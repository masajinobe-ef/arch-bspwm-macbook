{
    "autosave": 0.1,
    "autosu": true,
    "backup": false,
    "backupdir": "~/.cache/micro/backups",
    "basename": true,
    "colorscheme": "dracula",
    "diffgutter": true,
    "initlua": true,
    "matchbracestyle": "underline",
    "mkparents": true,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "savehistory": false,
    "scrollbar": true,
    "statusformatl": "$(filename) $(modified)($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:encoding)",
    "tabhighlight": true,
    "tabreverse": false,
    "xterm": true
}
